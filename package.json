{
  "name": "fx_frontend_template",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "copyassets": "cp -a assets/. build/assets && cp -a public-files/. build/public-files && cp -a html-templates/gssndata.php build/gssndata.php &&  chmod -R 776 ./build ",
    "watch:copyassets": "watch  'npm run copyassets' ./assets",
    "watch:html": "watch 'node ./templer.js --directory /html-templates/ --indexfile index.html --out build/index.html' html-templates",
    "autoprefixer": "postcss  -u autoprefixer -o ./build/main.css ./build/main.css",
    "watch:autoprefix": "watch 'npm run autoprefixer' build",
    "watch:styles": "node-sass -r -w --source-map-embed ./app/style/main.scss > postcss -u autoprefixer -o ./build",
    "start": "npm run build && http-server -p 9999",
    "build": "npm run webpackbuild && node ./templer.js --directory /html-templates/ --indexfile index.html --out build/index.html && node-sass -r ./app/style/main.scss > postcss -u autoprefixer -o ./build && npm run copyassets && npm run autoprefixer",
    "webpackbuild": "webpack"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.18.6",
    "@babel/preset-env": "^7.18.6",
    "autoprefixer": "^10.4.2",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.2.5",
    "css-loader": "^6.5.1",
    "file-loader": "^6.2.0",
    "html-loader": "^3.1.0",
    "node-noop": "^1.0.0",
    "node-sass": "^7.0.1",
    "optimist": "^0.6.1",
    "parallelshell": "^3.0.2",
    "postcss": "^8.4.5",
    "postcss-cli": "^9.1.0",
    "raw-loader": "^4.0.2",
    "sass-loader": "^12.4.0",
    "style-loader": "^3.3.1",
    "url-loader": "^4.1.1",
    "watch": "^1.0.2",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.9.2"
  },
  "dependencies": {
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "dynamics.js": "^1.1.5",
    "jquery": "^3.6.0",
    "js-cookie": "^3.0.1",
    "malihu-custom-scrollbar-plugin": "^3.1.5",
    "parallax-imagescroll": "^0.2.3"
  }
}
